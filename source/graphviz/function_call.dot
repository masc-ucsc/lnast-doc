digraph assign {
  rankdir=LR
  bgcolor="transparent"

  node  [fontname = "helvetica", shape=record, style="rounded", penwidth = 2];
  edge  [fontname = "helvetica", color="#142b30", arrowhead="normal", penwidth = 2];
  graph [fontname = "helvetica"];

  

  node0 [label = "<a> stmts | <s1> statement | <s2> statement | <s3> statement |  <s4> statement" ];
  node1 [label = "<a> func_def | <n> func_name | <cond> condition | <stmts> stmts |<io1> input | <io2> input |  <io3> output | <stmts> stmts"]
  node2 [label = "<a> ref   | <l> func_xor"];
  node3 [label = "<a> ref   | <l> $a"];
  node4 [label = "<a> ref   | <l> $b"];
  node5 [label = "<a> ref   | <l> %out"];
  node6 [label = "<a> stmts | <s1> statement | <s2> statement"];

  node7 [label = "<a> xor | <l> lhs | <r1> op1 | <r2> op2 "];
  node8 [label = "<a> ref   | <l> ___b"];
  node9 [label = "<a> ref   | <l> $a"];
  nodea [label = "<a> ref   | <l> $b"];

  nodeb [label = "<a> assign | <l> lhs  | <r> rhs"];
  nodec [label = "<a> ref   | <l> %out"];
  noded [label = "<a> ref   | <l> ___b"];

  nodee [label = "<a> condition | <l> true"];

  nodef [label = "<a> func_call | <l> lhs | <n> func_name | <i1> input | <i2>  input"];
  nodeg [label = "<a> ref | <l> my_xor"];
  nodeh [label = "<a> ref | <l> func_xor"];
  nodei [label = "<a> ref | <l> $foo"];
  nodej [label = "<a> ref | <l> $bar"];

  nodek [label = "<a> dot | <l> lhs | <r1> op1 | <r2> op2 "];
  nodel [label = "<a> ref   | <l> ___d"];
  nodem [label = "<a> ref   | <l> my_xor"];
  noden [label = "<a> ref   | <l> out"];

  nodeo [label = "<a> assign | <l> lhs  | <r> rhs"];
  nodep [label = "<a> ref   | <l> %out_top"];
  nodeq [label = "<a> ref   | <l> ___d"];



  node0:s1  -> node1:a;
  node0:s2  -> nodef:a;
  node0:s3  -> nodek:a;
  node0:s4  -> nodeo:a;

  node1:n   -> node2:a;
  node1:stmts -> node6:a;
  node1:cond  -> nodee:a;
  node1:io1 -> node3:a;
  node1:io2 -> node4:a;
  node1:io3 -> node5:a;

  node6:s1 -> node7:a;
  node6:s2 -> nodeb:a;

  node7:l -> node8:a;
  node7:r1 -> node9:a;
  node7:r2 -> nodea:a;
  
  nodeb:l -> nodec:a;
  nodeb:r -> noded:a;

  nodef:l -> nodeg:a;
  nodef:n -> nodeh:a;
  nodef:i1 -> nodei:a;
  nodef:i2 -> nodej:a;

  nodek:l  -> nodel:a;
  nodek:r1 -> nodem:a;
  nodek:r2 -> noden:a;

  nodeo:l -> nodep:a;
  nodeo:r -> nodeq:a;
}
