digraph assign {
  rankdir=LR
  bgcolor="transparent"

  node [shape=record, style="rounded", penwidth = 2];
  edge [color="#142b30", arrowhead="vee", penwidth = 2];


  node0 [label = "<a> assign | <l> lhs  | <r> rhs"];
  node1 [label = "<a> ref   | <l> string"];
  node2 [label = "<a> const | <l> string"];

  node0:l -> node1:a;
  node0:r -> node2:a;
}
